<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>
<div style="background-color: #80f0cd; width: 300px; margin: 40px auto; border-radius: 10px; padding: 5px ">
    {{#each user}}
        <div>FirstName: {{this.firstName}}</div>
        <div>LastName: {{this.lastName}}</div>
        <div>Age: {{this.age}}</div>
        <div>City: {{this.city}}</div>
        <div>Email: {{this.email}}</div>
        <div>Password: {{this.password}}</div>
    {{/each}}
</div>
</body>
</html>